func fiboIter(n:int) -> int {
	var a,b = (0,1)
	while (n > 1) {
		var tmp = a
		a = b
		b = tmp + b
		n = n - 1
	}
	return b
}

func fiboRec(n:int) -> int {
	if (n <= 0) {
		return 0
	}
	if (n == 1) {
		return 1
	}
	return fiboRec(n-2) + fiboRec(n-1)
}

var fi = fiboIter(10)
var fr = fiboRec(10)
print fi
print fr


